%term <__int_field__> NUMBER.
%nonterm <__int_field__> expr, <__int_field__> addendum,
 <__int_field__> factor.
program = statement SEMICOLON program | .
statement = PRINT out outlist.
outlist= COMMA out outlist  | .
out =  expr %{ printf("%ld\n", $1); %}.
expr = addendum %{ $$=$1 %} { PLUS addendum %{ $$+=$1 %} 
			| MINUS addendum %{ $$-=$1 %} } 
addendum = factor %{ $$=$1 %} { MULTIPLY factor %{ $$*=$1 %} 
			| DIVIDE factor %{ $$/=$1 %} }
factor = LPAREN expr RPAREN %{ $$=$1 %}
 			| NUMBER %{ $$=$1 %}
			| MINUS LPAREN expr RPAREN %{ $$=-$1 %}.