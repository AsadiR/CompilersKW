lex.yy.o: lex.yy.c
        g++ -c -o lex.yy.o lex.yy.c
Generator.o: Generator.cpp
        g++ -c -o Generator.o Generator.cpp
Node.o: Node.cpp
        g++ -c -o Node.o Node.cpp
Parser.o: Parser.cpp
        g++ -c -o Parser.o Parser.cpp       
            
hello: lex.yy.o Generator.o Node.o Parser.o
        g++ -s -pipe -o Generator lex.yy.o Generator.o Node.o Parser.o