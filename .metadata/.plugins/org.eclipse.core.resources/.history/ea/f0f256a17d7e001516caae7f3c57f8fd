#include <iostream>
#include <string>
#include "ArrayWithState.h"

using namespace std;

int align(string s[], int n) {
	ArrayWithState *array = new ArrayWithState(s, );
}

int main() {
	int n;
	int i;
	cin >> n;
	string s[n];
	int str_length_array[n];
	for (i=0; i<n; i++) {
		cin >> s[i];
		str_length_array[n] = s[i].length();
	}




	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!


	return 0;
}


