#include "BlossumMatrix.h"

BlossumMatrix::BlossumMatrix(map<char,int> charToInt, int** matrix) {
	this.charToInt = charToInt;
	this.matrix = matrix;
}

int BlossumMatrix::getValue(char sym1, char sym2) {
	return charToInt.at()
}

